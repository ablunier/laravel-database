{
    "name": "ablunier/laravel-database",
    "description": "Laravel database utilities package. Repository, cache, abstraction layer.",
    "keywords": ["laravel", "eloquent", "database", "repository", "cache", "abstraction"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian P. Blunier",
            "email": "adrian@blunier.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/database": "5.1.*",
        "doctrine/dbal": "2.5.*"
    },
    "require-dev": {
        "illuminate/database": "^5.1",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "ANavallaSuiza\\Laravel\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Database\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
